<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-configuration PUBLIC
	"-//Hibernate/Hibernate Configuration DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-configuration-3.0.dtd">
<hibernate-configuration>
	<session-factory>
		<property name="hibernate.connection.username">root</property>
		<property name="hibernate.connection.url">jdbc:mysql://localhost:3306/project</property>
		<property name="hibernate.connection.password">arun</property>
		<property name="hibernate.connection.driver_class">com.mysql.jdbc.Driver</property>
		<property name="hibernate.show_sql">true</property>
		<property name="hbm2ddl.auto">update</property>
		
		<mapping class="com.oehm2.assosiations.dto.Questions"/>
		<mapping class="com.oehm2.assosiations.dto.Answers"/>
		
		<mapping class="com.oehm2.assosiations.dto.State" />
		 <mapping class="com.oehm2.assosiations.dto.Country" />
		 
		<mapping class="com.oehm2.assosiations.dto.Team" />
		<mapping class="com.oehm2.assosiations.dto.Captain" />
		<mapping class="com.oehm2.assosiations.dto.Library" />
		<mapping class="com.oehm2.assosiations.dto.Book" />

	</session-factory>
</hibernate-configuration>